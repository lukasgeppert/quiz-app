    "info": {
        "title": "NestJS API",
        "description": "The NestJS API description",
        "version": "1.0",
        "contact": {}
    },
    "tags": [],
    "servers": [],
    "components": {
        "securitySchemes": {
            "cookie": {
                "type": "http",
                "in": "header",
                "name": "auth-cookie",
                "description": "Cookies are set automatically. No need to add.",
                "scheme": "bearer"
            }
        },
        "schemas": {
            "UserEntity": {
                "type": "object",
                "properties": {
                    "role": {
                        "type": "string",
                        "enum": [
                            "ADMIN",
                            "USER"
                        ],
                        "default": "USER"
                    },
                    "id": {
                        "type": "number"
                    },
                    "email": {
                        "type": "string"
                    },
                    "firstName": {
                        "type": "string",
                        "nullable": true
                    },
                    "lastName": {
                        "type": "string",
                        "nullable": true
                    },
                    "image": {
                        "type": "string",
                        "nullable": true
                    },
                    "emailVerified": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "role",
                    "id",
                    "email",
                    "firstName",
                    "lastName",
                    "image",
                    "emailVerified"
                ]
            },
            "ValidationErrorDto": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number"
                    },
                    "message": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "error": {
                        "type": "string"
                    }
                },
                "required": [
                    "statusCode",
                    "message",
                    "error"
                ]
            },
            "CredentialLogin": {
                "type": "object",
                "properties": {
                    "password": {
                        "type": "string",
                        "minLength": 8,
                        "pattern": "/[@$!%*#?&]/",
                        "example": "Password@123",
                        "description": "password must contain atleast 1 special character, 1 upper case and 1 number"
                    },
                    "email": {
                        "type": "string"
                    }
                },
                "required": [
                    "password",
                    "email"
                ]
            }
        }
    }
