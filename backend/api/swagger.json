                        "schema": {
                            "minimum": 0,
                            "default": 0,
                            "type": "number"
                        }
                    },
                    {
                        "name": "take",
                        "required": false,
                        "in": "query",
                        "description": "Limit the number of results",
                        "schema": {
                            "minimum": 1,
                            "default": 100,
                            "type": "number"
                        }
                    },
                    {
                        "name": "sort",
                        "required": false,
                        "in": "query",
                        "description": "Sort by this field",
                        "schema": {
                            "default": "id",
                            "enum": [
                                "id",
                                "question",
                                "createdAt",
                                "updatedAt"
                            ],
                            "type": "string"
                        }
                    },
                    {
                        "name": "field",
                        "required": false,
                        "in": "query",
                        "description": "Filter by this field",
                        "schema": {
                            "default": "id",
                            "type": "string"
                        }
                    },
                    {
                        "name": "contains",
                        "required": false,
                        "in": "query",
                        "description": "Filter by this value",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "from",
                        "required": false,
                        "in": "query",
                        "description": "Filter by this date",
                        "schema": {
                            "format": "2021-01-01T11:59:59.999Z",
                            "type": "string"
                        }
                    },
                    {
                        "name": "to",
                        "required": false,
                        "in": "query",
                        "description": "Filter by this date",
                        "schema": {
                            "format": "2021-01-01T11:59:59.999Z",
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "",
