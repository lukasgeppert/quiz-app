                "name": "auth-cookie",
                "description": "Cookies are set automatically. No need to add.",
                "scheme": "bearer"
            }
        },
        "schemas": {
            "UserEntity": {
                "type": "object",
                "properties": {
                    "role": {
                        "type": "string",
                        "enum": [
                            "ADMIN",
                            "USER"
                        ],
                        "default": "USER"
                    },
                    "id": {
                        "type": "number"
                    },
                    "email": {
                        "type": "string"
                    },
                    "firstName": {
                        "type": "string",
                        "nullable": true
                    },
                    "lastName": {
                        "type": "string",
                        "nullable": true
                    },
                    "image": {
                        "type": "string",
                        "nullable": true
                    }
                },
                "required": [
                    "role",
                    "id",
                    "email",
                    "firstName",
                    "lastName",
                    "image"
                ]
            },
            "ValidationErrorDto": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number"
                    },
                    "message": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "error": {
                        "type": "string"
                    }
                },
                "required": [
                    "statusCode",
                    "message",
                    "error"
                ]
            },
            "CredentialLogin": {
                "type": "object",
                "properties": {
                    "password": {
                        "type": "string",
                        "minLength": 8,
                        "pattern": "/[@$!%*#?&]/",
                        "example": "Password@123",
                        "description": "password must contain atleast 1 special character, 1 upper case and 1 number"
                    },
                    "email": {
                        "type": "string"
                    }
                },
                "required": [
                    "password",
                    "email"
                ]
            },
            "CredentailRegister": {
                "type": "object",
                "properties": {
                    "password": {
                        "type": "string",
                        "minLength": 8,
                        "pattern": "/[@$!%*#?&]/",
                        "example": "Password@123",
                        "description": "password must contain atleast 1 special character, 1 upper case and 1 number"
                    },
                    "email": {
                        "type": "string"
                    },
                    "role": {
                        "type": "object",
                        "default": "USER"
                    }
                },
                "required": [
                    "password",
                    "email",
                    "role"
                ]
            },
            "CreateExamDto": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    }
                },
                "required": [
                    "name"
                ]
            },
            "ListExamEntity": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "number"
                    },
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "createdAt": {
                        "format": "date-time",
                        "type": "string"
                    },
                    "updatedAt": {
                        "format": "date-time",
                        "type": "string"
                    }
                },
                "required": [
                    "id",
                    "name",
                    "createdAt",
                    "updatedAt"
                ]
            },
            "TrueFalseEntity": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "number"
                    },
                    "type": {
                        "type": "object",
                        "default": "TRUE_FALSE"
                    },
                    "question": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string",
                        "nullable": true
                    },
                    "answer": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "id",
                    "type",
                    "question",
                    "description",
                    "answer"
                ]
            },
            "MultipleChoiceEntity": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "number"
                    },
                    "question": {
                        "type": "string"
                    },
                    "type": {
                        "type": "object",
                        "default": "MULTIPLE_CHOICE"
                    },
                    "description": {
                        "type": "string",
                        "nullable": true
                    },
                    "options": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "answer": {
                        "type": "number"
                    }
                },
                "required": [
                    "id",
                    "question",
                    "type",
                    "description",
                    "options",
                    "answer"
                ]
            },
            "MultipleSelectEntity": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "number"
                    },
                    "question": {
                        "type": "string"
                    },
                    "type": {
                        "type": "object",
                        "default": "MULTIPLE_SELECT"
                    },
                    "description": {
                        "type": "string",
                        "nullable": true
                    },
                    "options": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "answers": {
                        "type": "array",
                        "items": {
                            "type": "number"
                        }
                    }
                },
                "required": [
                    "id",
                    "question",
                    "type",
                    "description",
                    "options",
                    "answers"
                ]
            },
